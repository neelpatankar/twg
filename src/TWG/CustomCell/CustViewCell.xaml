<?xml version="1.0" encoding="UTF-8"?>
<ViewCell xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="TWG.CustomCell.CustViewCell" Height="140">
    <ViewCell.View>
        <Grid Margin="5">
           <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
             <ColumnDefinition Width="*"/>
          </Grid.ColumnDefinitions>
          <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
             <RowDefinition Height="*"/>
             <RowDefinition Height="*"/>
             <RowDefinition Height="*"/>
             <RowDefinition Height="*"/>
          </Grid.RowDefinitions>
           <Label Text="{Binding Name}"  FontAttributes="Bold" Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2"/>
           <Label Text="{Binding Var}" FontAttributes="Bold" Grid.Column="0" Grid.Row="1"/>
           <Label Text="{Binding B}" FontAttributes="Bold" Grid.Column="1" Grid.Row="1"/>
           <Label Text="{Binding DT}" FontAttributes="Bold" Grid.Column="0" Grid.Row="2"/>
           <Label Text="{Binding GT}" FontAttributes="Bold" Grid.Column="1" Grid.Row="2" />
           <Label Text="{Binding NT}" FontAttributes="Bold" Grid.Column="0" Grid.Row="3"/>
           <Label Text="{Binding BX}" FontAttributes="Bold" Grid.Column="1" Grid.Row="3"/>
           <Label Text="{Binding MOG}" FontAttributes="Bold" Grid.Column="0" Grid.RowSpan="2" Grid.Row="4"/>
        </Grid>
    </ViewCell.View>
</ViewCell>
